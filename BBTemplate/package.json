{
  "name": "bbtemplate",
  "displayName": "BBTemplate",
  "description": "Template Demo For Egret Wing.",
  "version": "0.0.3",
  "publisher": "egret",
  "engines": {
    "wing": "^3.0.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.bbtemplate"
   
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.bbtemplate",
        "title": "Egret-plug Template"
      }
    ]
 
  },
  "scripts": {
    "wing:prepublish": "node ./node_modules/egretwing/bin/compile",
    "compile": "node ./node_modules/egretwing/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/egretwing/bin/install"
  },
  "devDependencies": {
    "egretwing": "^3.0.0",
    "typescript": "^1.7.5",
    "mml-emitter":"^1.1.0"
  }
}
